{
  "name": "intelliwatt-website",
  "version": "1.0.0",
  "description": "IntelliWattâ„¢ - Stop Overpaying for Power",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "admin:summary": "tsx ./scripts/admin/smoke.ts summary",
    "admin:catch": "tsx ./scripts/admin/smoke.ts catch",
    "address:esiid": "tsx ./scripts/admin/resolve.ts address",
    "esiid:meter": "tsx ./scripts/admin/resolve.ts esiid",
    "esiid:resolve-save": "tsx ./scripts/admin/esiid-save.ts resolve-save",
    "esiid:save": "tsx ./scripts/admin/esiid-save.ts save",
    "test:connections": "tsx ./scripts/admin/test-connections.ts",
    "test:prod": "node scripts/admin/api_test_prod.mjs --base",
    "check:ercot": "node scripts/db/check_ercot_setup.mjs",
    "check:ercot:local": "node scripts/db/check_ercot_setup.mjs",
    "ercot:test": "tsx scripts/ercot/test_match.ts",
    "ercot:test:local": "tsx scripts/ercot/test_match.ts --",
    "ercot:load:file": "tsx scripts/ercot/load_from_file.ts --",
    "ercot:fetch:latest": "tsx scripts/ercot/fetch_latest.ts --",
    "ercot:cron:curl": "bash -lc 'curl -sS \"http://localhost:3000/api/admin/ercot/cron?token=$CRON_SECRET\"'",
    "deploy:prod": "vercel --prod --confirm",
    "smoke:prod": "node scripts/deploy/smoke_prod.mjs",
    "analysis:daily:csv": "tsx scripts/admin/export_daily_summary.ts --",
    "plans:ingest:wattbuy": "echo 'Use curl with x-admin-token to /api/admin/wattbuy/ingest (see QUICK_START)'",
    "plans:estimate": "echo 'Use curl to POST /api/plan/estimate (see QUICK_START)'",
    "test:ercot:fetch": "node scripts/admin/test_ercot_fetch.mjs",
    "ercot:url:sanity": "node scripts/admin/ercot_url_sanity.mjs",
    "ercot:ingests": "powershell -NoProfile -Command \"Invoke-WebRequest -Headers @{ 'x-admin-token'=$env:ADMIN_TOKEN } -Uri $env:PROD_BASE_URL'/api/admin/ercot/ingests?limit=10' -UseBasicParsing | Select-Object -ExpandProperty Content\"",
    "ercot:resolve-fetch": "node scripts/admin/resolve_and_fetch.mjs"
  },
  "dependencies": {
    "@google-cloud/vision": "^5.3.4",
    "@prisma/client": "^6.11.1",
    "@types/js-cookie": "^3.0.6",
    "@types/nodemailer": "^6.4.17",
    "@types/papaparse": "^5.3.16",
    "cheerio": "^1.1.2",
    "dotenv": "^16.4.5",
    "html-to-text": "^9.0.5",
    "js-cookie": "^3.0.5",
    "luxon": "^3.7.2",
    "next": "^14.0.0",
    "nodemailer": "^7.0.4",
    "papaparse": "^5.5.3",
    "pdf-parse": "^2.1.6",
    "pdfjs-dist": "^5.4.149",
    "pg": "^8.11.5",
    "prisma": "^6.11.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/luxon": "^3.7.1",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.15.6",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "tsx": "^4.19.2",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "energy",
    "electricity",
    "savings",
    "utility",
    "smart-meter"
  ],
  "author": "IntelliPath Solutions",
  "license": "MIT"
}
